
public class Iteration {

	public static void main(String[] args) {
		// System.out.println(Parameters(3, 3, true));

		for (int i = 0; i < 10; i++) {
			System.out.println(Parameters(i, 3, true));
		}

		int[] tenNumbers = new int[10]; // create array

		for (int i : tenNumbers) { // write 9, 10 times
			i = 9;
		
		}

		for (int i : tenNumbers) {

//			System.out.println(Parameters(1, 2, true));

//			System.out.println(Parameters(18989, 289, true));

//			System.out.println(Parameters(1, 2, false));

			System.out.println(Parameters(0, i, true)); // 

		}

	}

	public static int Parameters(int number1, int number2, boolean test) {
		System.out.println(number2 + "woo");
		if (number1 == 0) {
			return number2;
		}

		if (number2 == 0) {
			return number1;
		}

		if (test) {
			return number1 + number2;
		}

		else {
			return number1 * number2;
		}

	}

}
