
public class HelloWorld {
	
	public static void main(String[] args)
	{
		System.out.println(Parameters(2, 3, true));
	}
	
	public static int Parameters (int number1, int number2, boolean test)
	{
		int sum;
		if (test) {
			sum=number1+number2;
			//System.out.println(number1 + number2);
			}
		else {
			sum=number1*number2;
			//System.out.println(number1 * number2);
		}
		return sum;
		
	}
}

