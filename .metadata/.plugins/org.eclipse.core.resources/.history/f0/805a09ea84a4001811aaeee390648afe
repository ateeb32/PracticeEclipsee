
public class Conditionals {
	
	public void main2(int number1, int number2, int sum, int multiply)
	{
		number1=2;
		number2=3;
		sum=number1+number2;
		multiply=number1*number2;
		
		boolean sumMultiply = true;
		
		if (sumMultiply) {
			System.out.println("The sum of the two numbers are" + sum);
		}
		
		System.out.println(number1);
		System.out.println("+");
		System.out.println(number2);
	}

}
